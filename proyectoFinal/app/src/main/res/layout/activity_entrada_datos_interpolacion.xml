<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.numericos.proyectofinal.views.interpolation.Entrada_datos_interpolacion">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Points X"
        android:id="@+id/textView22"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Points Y"
        android:id="@+id/textView23"
        android:layout_marginTop="62dp"
        android:layout_below="@+id/textView22"
        android:layout_alignLeft="@+id/textView22"
        android:layout_alignStart="@+id/textView22" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/puntosx"
        android:layout_alignBottom="@+id/textView22"
        android:layout_alignLeft="@+id/puntosy"
        android:layout_alignStart="@+id/puntosy" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/puntosy"
        android:layout_above="@+id/textView24"
        android:layout_centerHorizontal="true"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Value to\nInterpolate"
        android:id="@+id/textView24"
        android:layout_marginTop="57dp"
        android:layout_below="@+id/textView23"
        android:layout_toLeftOf="@+id/puntosinterpolar"
        android:layout_toStartOf="@+id/puntosinterpolar" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/puntosinterpolar"
        android:layout_alignBottom="@+id/textView24"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Value of T"
        android:id="@+id/textView25"
        android:layout_marginTop="47dp"
        android:layout_below="@+id/textView24"
        android:layout_alignLeft="@+id/textView24"
        android:layout_alignStart="@+id/textView24" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/valorT"
        android:layout_alignBottom="@+id/textView25"
        android:layout_alignRight="@+id/puntosinterpolar"
        android:layout_alignEnd="@+id/puntosinterpolar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Points F(x)"
        android:id="@+id/textView26"
        android:layout_marginTop="51dp"
        android:layout_below="@+id/textView25"
        android:layout_alignLeft="@+id/textView25"
        android:layout_alignStart="@+id/textView25" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/puntosf"
        android:layout_alignBottom="@+id/textView26"
        android:layout_alignRight="@+id/valorT"
        android:layout_alignEnd="@+id/valorT" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continuar"
        android:id="@+id/button11"
        android:layout_below="@+id/textView26"
        android:layout_toRightOf="@+id/textView25"
        android:layout_toEndOf="@+id/textView25"
        android:layout_marginTop="42dp"
        android:onClick="continuar"/>

</RelativeLayout>
