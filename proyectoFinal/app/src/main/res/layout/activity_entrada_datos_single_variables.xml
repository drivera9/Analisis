        android:id="@+id/textView21"
        android:layout_above="@+id/datoFxP_single_variables"
        android:layout_toRightOf="@+id/ok_datos"
        android:layout_toEndOf="@+id/ok_datos" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:id="@+id/datoFxPP_single_variables"
        android:layout_alignTop="@+id/datoFxP_single_variables"
        android:layout_alignLeft="@+id/textView21"
